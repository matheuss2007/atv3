13- Fibonacci
#include <stdio.h>

int main() {
    int N;
    
    printf("Digite o número de termos da sequência de Fibonacci: ");
    scanf("%d", &N);

    if (N <= 0) {
        printf("O número de termos deve ser positivo.\n");
        return 1;
    }

    unsigned long long int a = 0, b = 1, c;

    printf("Sequência de Fibonacci com %d termos:\n", N);
    for (int i = 0; i < N; i++) {
        if (i == 0) {
            printf("%llu ", a);
        } else if (i == 1) {
            printf("%llu ", b);
        } else {
            c = a + b;
            a = b;
            b = c;
            printf("%llu ", c);
        }
    }
    printf("\n");

    return 0;
}
