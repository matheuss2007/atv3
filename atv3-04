4- Maior de Dois Números #include <stdio.h>

int main (){ int numero1; int numero2;

printf("digite um numero:");
scanf("%d", &numero1);
printf("digite outro numero:");
scanf("%d", &numero2);

  if (numero1 > numero2) {
    printf("O número %d e maior\n", numero1);
} else {
    printf("O número %d é maior\n", numero2);
}
